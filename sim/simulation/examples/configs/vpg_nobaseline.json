    "saver": {
        "directory": null,
        "seconds": 600
    },
    "summarizer": {
        "directory": null,
        "labels": ["graph", "total-loss"]
    },
    "execution": {
        "type": "single",
        "session_config": null,
        "distributed_spec": null
    }
